@model List<UserInformation>

@{
    ViewData["Title"] = "User Information List";
}

<h1>@ViewData["Title"]</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>DOB</th>
            <!-- Add more columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Name</td>
                <td>@user.Email</td>
                <td>@user.DOB</td>
                <!-- Add more fields as needed -->
            </tr>
        }
    </tbody>
</table>
